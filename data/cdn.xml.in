<?xml version='1.0'?>
<mime-info xmlns='http://www.freedesktop.org/standards/shared-mime-info'>
  <mime-type type="text/x-cdn">
    <comment>Codyn File</comment>
    <comment xml:lang="nl">Codyn bestand</comment>
    <!-- more translated comment elements -->
    <glob pattern="*.cdn"/>
    <sub-class-of type="text/plain"/>
    <generic-icon name="document"/>
    <magic priority="50">
      <match type="string" offset="0" value="#!/usr/bin/cdn-parser"/>
      <match type="string" offset="0" value="#!/usr/local/bin/cdn-parser"/>
      <match type="string" offset="0" value="#!@prefix@/bin/cdn-parser"/>
      <match type="string" offset="0" value="#!/usr/bin/cdn-monitor"/>
      <match type="string" offset="0" value="#!/usr/local/bin/cdn-monitor"/>
      <match type="string" offset="0" value="#!@prefix@/bin/cdn-monitor"/>
    </magic>
  </mime-type>
</mime-info>
