network
{
	## Gravity constant
	g ?= "-9.81"
}

templates
{
	group "physics"
	{
		## The body template represents a single mass in the system.
		## It needs to implement the functions X and Y (as functions of
		## the generalized coordinates) representing the mass
		## position
		group "body"
		{
			## Body mass
			m = 1
		}

		group "spring"
		{
			## Spring constant
			k = 1
		}
	}
}
